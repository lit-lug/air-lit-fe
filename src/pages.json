{
  "globalStyle": {
    "backgroundColor": "@bgColor",
    "backgroundColorBottom": "@bgColorBottom",
    "backgroundColorTop": "@bgColorTop",
    "backgroundTextStyle": "@bgTxtStyle",
    "navigationBarBackgroundColor": "#0000",
    "navigationBarTextStyle": "@navTxtStyle",
    "navigationBarTitleText": "云小洛",
    "navigationStyle": "custom"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "时刻",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/calendar/calendar",
      "style": {
        "navigationBarTitleText": "校历",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/level/level",
      "style": {
        "navigationBarTitleText": "等级成绩",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/ring/ring",
      "style": {
        "navigationBarTitleText": "实践环节",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/elec/elec",
      "style": {
        "navigationBarTitleText": "宿舍用电",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/score/score",
      "style": {
        "navigationBarTitleText": "考试成绩",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/auth/auth",
      "style": {
        "navigationBarTitleText": "授权中...",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/webview/webview",
      "style": {
        "navigationBarTitleText": "详情",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/mine/mine",
      "style": {
        "navigationBarTitleText": "我的",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/mine/account/account",
      "style": {
        "navigationBarTitleText": "账户管理",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/ggtz/ggtz",
      "style": {
        "navigationBarTitleText": "公告通知",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/ggtz/detail/detail",
      "style": {
        "navigationBarTitleText": "通知详情",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/mine/setting/setting",
      "style": {
        "navigationBarTitleText": "公众号配置",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/mine/changelog/changelog",
      "style": {
        "navigationBarTitleText": "更新日志",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/mine/about/about",
      "style": {
        "navigationBarTitleText": "关于我们",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/schedule/schedule",
      "style": {
        "navigationBarTitleText": "课表",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/schedule/detail/detail",
      "style": {
        "navigationBarTitleText": "课程详情",
        "enablePullDownRefresh": true
      }
    }
  ],
  "tabBar": {
    "color": "@bgColor",
    "selectedColor": "@selectedColor",
    "backgroundColor": "@tabBgColor",
    "position": "bottom",
    "borderStyle": "@tabBorderStyle",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/img/index.png",
        "selectedIconPath": "static/img/index-s.png",
        "text": "时刻"
      },
      {
        "pagePath": "pages/schedule/schedule",
        "iconPath": "static/img/schedule.png",
        "selectedIconPath": "static/img/schedule-s.png",
        "text": "课表"
      },
      {
        "pagePath": "pages/mine/mine",
        "iconPath": "static/img/mine.png",
        "selectedIconPath": "static/img/mine-s.png",
        "text": "我的"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^U(.*)": "~/components/UnoUI/U$1/U$1.vue",
      "^A(.*)": "ano-ui/components/A$1/A$1.vue"
    }
  }
}
